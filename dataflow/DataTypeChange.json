{
	"name": "DataTypeChange",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "select2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {Trader No} as string,",
				"          {Order No} as string,",
				"          {Company No} as string,",
				"          ORD_EMP_NO as string,",
				"          {Customer No.} as string,",
				"          ORD_CRTE_DTIME as string,",
				"          {Order Amt} as integer,",
				"          Order_Source as string,",
				"          Order_Source_Type as string,",
				"          Booking_Date as date,",
				"          Link_Date as date,",
				"          Booking_Status as string,",
				"          TEAM_LEADER as string,",
				"          Team_leader_Joining_Date as date,",
				"          Team_leader_Leave_Date as date,",
				"          KEC_Join_Date as date,",
				"          KEC_Leave_Date as date,",
				"          Team_Leader_Name as string,",
				"          Team_Leader_Code as string,",
				"          {Active/Inactive} as string,",
				"          {Lead No} as string,",
				"          Lead_TYPE as string,",
				"          Lead_EMP_NO as string,",
				"          Lead_CRTE_DTIME as timestamp,",
				"          {Lead source type} as string,",
				"          Lead_Sub_Source_Name as string,",
				"          Lead_Source as string,",
				"          Lead_Category_Type as string,",
				"          Booking_Qty as string,",
				"          Invoice_No as string,",
				"          INVC_TYPE as string,",
				"          Invoice_Date as date 'dd-MM-yyyy',",
				"          Delivery_Date as date 'dd-MM-yyyy',",
				"          Lead_Status as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> source1",
				"source1 select(mapColumn(",
				"          {Trader No},",
				"          {Order No},",
				"          {Company No},",
				"          ORD_EMP_NO,",
				"          {Customer No.},",
				"          ORD_CRTE_DTIME,",
				"          {Order Amt},",
				"          Order_Source,",
				"          Order_Source_Type,",
				"          Link_Date,",
				"          Booking_Status,",
				"          TEAM_LEADER,",
				"          Team_leader_Joining_Date,",
				"          Team_leader_Leave_Date,",
				"          KEC_Join_Date,",
				"          KEC_Leave_Date,",
				"          Team_Leader_Name,",
				"          Team_Leader_Code,",
				"          {Active/Inactive},",
				"          {Lead No},",
				"          Lead_TYPE,",
				"          Lead_EMP_NO,",
				"          Lead_CRTE_DTIME,",
				"          {Lead source type},",
				"          Lead_Sub_Source_Name,",
				"          Lead_Source,",
				"          Lead_Category_Type,",
				"          Booking_Qty,",
				"          Invoice_No,",
				"          INVC_TYPE,",
				"          Invoice_Date,",
				"          Delivery_Date,",
				"          Lead_Status",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 derive(Flag = 'Order') ~> derivedColumn1",
				"derivedColumn1 select(mapColumn(",
				"          {Trader No},",
				"          {Order No},",
				"          {Company No},",
				"          ORD_EMP_NO,",
				"          {Customer No.},",
				"          ORD_CRTE_DTIME,",
				"          {Order Amt},",
				"          Order_Source,",
				"          Order_Source_Type,",
				"          Link_Date,",
				"          Booking_Status,",
				"          TEAM_LEADER,",
				"          Team_leader_Joining_Date,",
				"          Team_leader_Leave_Date,",
				"          KEC_Join_Date,",
				"          KEC_Leave_Date,",
				"          Team_Leader_Name,",
				"          Team_Leader_Code,",
				"          {Active/Inactive},",
				"          {Lead No},",
				"          Lead_TYPE,",
				"          Lead_EMP_NO,",
				"          Lead_CRTE_DTIME,",
				"          {Lead source type},",
				"          Lead_Sub_Source_Name,",
				"          Lead_Source,",
				"          Lead_Category_Type,",
				"          Booking_Qty,",
				"          Invoice_No,",
				"          INVC_TYPE,",
				"          Invoice_Date,",
				"          Delivery_Date,",
				"          Lead_Status,",
				"          Flag",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2"
			]
		}
	}
}